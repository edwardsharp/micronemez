<table>
  <tr>
    <th>title</th>
    <th>description</th>
    <th>live</th>
    <th>channel</th>
    <th>status</th>
    <th>start</th>
    <th>end</th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= link_to schedule.title, schedule %></td>
    <td><%= schedule.description %></td>
    <td><%= schedule.live %></td>
    <td><%= schedule.channel %></td>
    <td><%= schedule.status %></td>
    <td><%= schedule.start %></td>
    <td><%= schedule.end %></td>
    <% if user_signed_in? %>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, confirm: 'Are you sure?', method: :delete %></td>
    <% end %>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'new schedule', new_schedule_path %>
