<dl>
  <dt>email</dt>
  <dd><%= @user.email %></dd>

  <dt>sign in count</dt>
  <dd><%= @user.sign_in_count %></dd>

  <dt>last sign in at</dt>
  <dd><%= @user.last_sign_in_at.to_s(:joined) %></dd>

  <dt>last sign in IP</dt>
  <dd><%= @user.last_sign_in_ip %></dd>

  <dt>created at</dt>
  <dd><%= @user.created_at.to_s(:joined) %></dd>

  <dt>updated at</dt>
  <dd><%= @user.updated_at.to_s(:joined) %></dd>

</dl>
